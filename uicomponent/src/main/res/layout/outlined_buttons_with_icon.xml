<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/margin_small"
    android:background="@color/mint"
    android:orientation="vertical"
    android:padding="@dimen/margin_small">

    <Button
        style="@style/Widget.Moadgara.Button.Outlined"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_tiny"
        android:text="Outlined Iconed"
        app:icon="@drawable/abc_vector_test" />

    <Button
        style="@style/Widget.Moadgara.Button.Outlined.Rounded.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_tiny"
        app:icon="@drawable/abc_vector_test"
        android:text="Outlined Iconed Rounded Small" />

    <Button
        style="@style/Widget.Moadgara.Button.Outlined.Rounded.Medium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_tiny"
        app:icon="@drawable/abc_vector_test"
        android:text="Outlined Iconed Rounded Medium" />

    <Button
        style="@style/Widget.Moadgara.Button.Outlined.Rounded.High"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_tiny"
        app:icon="@drawable/abc_vector_test"
        android:text="Outlined Iconed Rounded High" />
</LinearLayout>